{
    "name": "g.maissa/vm-dashboard",
    "description": "Virtual machines dashboard backend",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "CEM\\": "src/CEM"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CEM\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GMaissa/BehatContextsExtension.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/GMaissa/RedmineUserProviderBundle.git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^3.2",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "twig/twig": "^1.0||^2.0",
        "friendsofsymfony/oauth-server-bundle": "^1.5",
        "aws/aws-sdk-php-symfony": "^1.2",
        "jms/serializer-bundle": "^1.3",
        "friendsofsymfony/rest-bundle": "^2.1",
        "nelmio/api-doc-bundle": "^2.13",
        "nelmio/cors-bundle": "^1.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "dev-master@dev",
        "squizlabs/php_codesniffer": "^2.8",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phpunit/phpunit": "^6.0",
        "pdepend/pdepend": "^2.5",
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "behatch/contexts": "^2.6",
        "phpunit/php-code-coverage": "^5.0",
        "theofidry/alice-bundle-extension": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "gmaissa/behat-contexts-extension": "dev-master"
    },
    "scripts": {
        "build-params": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
